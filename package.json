  "author": {
    "name": "Paul Shryock",
    "email": "paul@pshry.com",
    "url": "https://pshry.com/"
  },
  "scripts": {
    "start": "npm run build",
    "test": "npm-run-all lint audit debug",
    "prebuild": "npm-run-all clean webpack",
    "build": "npm run eleventy",
    "postbuild": "npm-run-all clean:css clean:fonts clean:img clean:js",
    "watch": "npm run eleventy:watch",
    "preserve": "npm run prebuild",
    "serve": "npm run eleventy:serve",
    "update": "ncu -u && npm update",
    "lint": "npm run standard:fix",
    "debug": "npm run eleventy:debug",
    "audit": "npm audit && npm run lighthouse",
    "standard": "npx standard",
    "standard:fix": "npx standard --fix",
    "lighthouse": "lighthouse http://localhost:8081 --budget-path=performance-budget.json --view",
    "clean": "npm-run-all clean:*",
    "clean:build": "if [ -d build ]; then rm -R build; fi",
    "clean:css": "if [ -d css ]; then rm -R css; fi",
    "clean:fonts": "if [ -d fonts ]; then rm -R fonts; fi",
    "clean:img": "if [ -d img ]; then rm -R img; fi",
    "clean:js": "if [ -d js ]; then rm -R js; fi",
    "webpack": "npx webpack",
    "webpack:watch": "npx webpack --watch",
    "eleventy": "npx eleventy",
    "eleventy:debug": "DEBUG=Eleventy* npm run eleventy --dryrun",
    "eleventy:watch": "npx eleventy --watch",
    "eleventy:serve": "npx eleventy --serve"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.8.3",
    "@babel/core": "^7.5.5",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.5.5",
    "@babel/register": "^7.5.5",
    "autoprefixer": "^9.6.1",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.1.0",
    "cssnano": "^4.1.10",
    "file-loader": "^4.1.0",
    "html-minifier": "^4.0.0",
    "image-webpack-loader": "^5.0.0",
    "lighthouse": "^5.2.0",
    "mini-css-extract-plugin": "^0.8.0",
    "npm-run-all": "^4.1.5",
    "postcss-easy-import": "^3.0.0",
    "postcss-loader": "^3.0.0",
    "postcss-node-sass": "^2.1.8",
    "postcss-preset-env": "^6.7.0",
    "precss": "^4.0.0",
    "standard": "^13.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.39.1",
    "webpack-cli": "^3.3.6"
  },
  "dependencies": {
    "@sendgrid/mail": "^6.4.0",
    "dotenv": "^8.0.0",
    "papaparse": "^5.0.1"
  }
}
