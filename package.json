{
  "name": "npsk",
  "description": "Fork this repository to start a new project with Eleventy, Netlify CMS, Gulp, Babel, PostCSS, and the following workflow: linting, Sass and modern CSS transpiling, ES2015+ compiling, concatenation, minification, beautification, source maps, bundling fonts, images, and favicons, serving files, watching for changes, and live reloading. Includes common accessible HTML, CSS, and JS components, and deployment configurations for Netlify and Zeit Now.",
  "keywords": [
    "eleventy",
    "eleventy-starter"
  ],
  "version": "0.27.0",
  "license": "Hippocratic License v1.0",
  "author": {
    "name": "Paul Shryock",
    "email": "paul@pshry.com",
    "url": "https://pshry.com/"
  },
  "homepage": "https://newprojectstarterkit.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paulshryock/New-Project-Starter-Kit.git"
  },
  "bugs": {
    "url": "https://github.com/paulshryock/New-Project-Starter-Kit/issues"
  },
  "scripts": {
    "start": "npm run build",
    "build": "NODE_ENV='production' npx gulp build && NODE_ENV='production' npx eleventy",
    "develop": "NODE_ENV='development' npx gulp develop && NODE_ENV='development' npx eleventy",
    "watch": "NODE_ENV='development' npx eleventy --watch",
    "serve": "NODE_ENV='development' npx gulp serve",
    "now-build": "npm start",
    "audit:performance": "node ./_scripts/lighthouse.js",
    "create:content": "node ./_scripts/addContentType.js",
    "update:gitignore": "node ./_scripts/ignorePlatforms.js"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.9.0",
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "autoprefixer": "^9.6.4",
    "babel-loader": "^8.0.6",
    "chrome-launcher": "^0.11.2",
    "cssnano": "^4.1.10",
    "del": "^5.1.0",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-beautify": "^3.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-connect": "^5.7.0",
    "gulp-htmlmin": "^5.0.1",
    "gulp-postcss": "^8.0.0",
    "gulp-rename": "^1.4.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-standard": "^14.0.0",
    "gulp-stylelint": "^9.0.0",
    "gulp-uglify": "^3.0.2",
    "lighthouse": "^5.5.0",
    "pixrem": "^5.0.0",
    "postcss-easy-import": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "precss": "^4.0.0",
    "stylelint": "^11.1.1",
    "stylelint-config-standard": "^19.0.0",
    "webpack": "^4.41.1",
    "webpack-cli": "^3.3.9",
    "webpack-stream": "^5.2.1"
  },
  "dependencies": {
    "@sendgrid/mail": "^6.4.0",
    "config": "^3.2.3",
    "debug": "^4.1.1",
    "papaparse": "^5.1.0"
  },
  "browserslist": [
    "last 3 versions",
    "> 1%",
    "maintained node versions",
    "not dead"
  ]
}
