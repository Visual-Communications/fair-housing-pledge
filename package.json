{
  "name": "New-Project-Starter-Kit",
  "title": "New Project Starter Kit",
  "version": "0.19.0",
  "description": "Start from scratch without reinventing the wheel: webpack, Babel, Sass, PostCSS, Eleventy, PWA, service worker",
  "url": "https://example.com",
  "company": "Example Corp",
  "private": true,
  "scripts": {
    "start": "npm run build",
    "clean": "npm run clean:dist & npm run clean:js & npm run clean:css & npm run clean:fonts & npm run clean:img",
    "clean:dist": "if [ -e dist ]; then rm -R dist; fi",
    "clean:js": "if [ -e js ]; then rm -R js; fi",
    "clean:css": "if [ -e css ]; then rm -R css; fi",
    "clean:fonts": "if [ -e fonts ]; then rm -R fonts; fi",
    "clean:img": "if [ -e img ]; then rm -R img; fi",
    "webpack": "npx webpack",
    "webpack:watch": "npx webpack --watch",
    "eleventy": "npx eleventy",
    "eleventy:watch": "npx eleventy --watch",
    "eleventy:serve": "npx eleventy --serve --port=8081",
    "prebuild": "npm run clean && npm run webpack",
    "build": "npm run eleventy",
    "prewatch": "npm run prebuild",
    "watch": "npm run eleventy:watch",
    "preserve": "npm run prebuild",
    "serve": "npm run eleventy:serve",
    "now-build": "npm start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paulshryock/New-Project-Starter-Kit.git"
  },
  "keywords": [],
  "author": {
    "name": "Paul Shryock",
    "email": "paul@pshry.com",
    "url": "https://pshry.com/"
  },
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/paulshryock/New-Project-Starter-Kit/issues"
  },
  "homepage": "https://github.com/paulshryock/New-Project-Starter-Kit#readme",
  "devDependencies": {
    "@11ty/eleventy": "^0.7.1",
    "@babel/core": "^7.3.4",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.4",
    "@babel/register": "^7.0.0",
    "autoprefixer": "^9.5.0",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.1",
    "cssnano": "^4.1.10",
    "html-minifier": "^3.5.21",
    "image-webpack-loader": "^4.6.0",
    "mini-css-extract-plugin": "^0.5.0",
    "postcss-loader": "^3.0.0",
    "postcss-node-sass": "^2.1.8",
    "postcss-preset-env": "^6.6.0",
    "precss": "^4.0.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0"
  }
}
