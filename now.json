  "alias": ["newprojectstarterkit.com"],
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build",
      "config": {
        "distDir": "build/api"
      }
    }
  ],
  "env": {
    "ELEVENTY_ENV": "@ELEVENTY_ENV",
