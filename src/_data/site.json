{
  "title": "{{ pkg.title }}",
  "description": "{{ pkg.description }}",
  "url": "{{ pkg.url }}",
  "version": "{{ pkg.version }}",

  "logo": {
    "url": {
      "k": "/img/logos/visual-communications.svg",
      "ko": "/img/logos/visual-communications-ko.svg"
    },
    "width": 180,
    "height": 72
  },

  "feed": {
    "subtitle": "{{ pkg.title }}",
    "filename": "feed.xml",
    "path": "/feed/feed.xml",
    "url": "{{ pkg.url }}/feed/feed.xml",
    "id": "{{ pkg.url }}/"
  },

  "author": {
    "name": "{{ pkg.author.name }}",
    "email": "{{ pkg.author.email }}",
    "url": "{{ pkg.author.url }}"
  },

  "layout": "page",
  "content_type": "page",

  "copyright": "&copy {{ 'now' | date: '%Y' }} {{ pkg.company }}. All rights reserved."
}